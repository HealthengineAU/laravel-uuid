{
    "name": "healthengine/laravel-uuid",
    "description": "Use UUIDs as primary keys for Eloquent models",
    "keywords": ["laravel", "eloquent", "uuid"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jarryd Tilbrook",
            "email": "jrad.tilbrook@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "~5.6|~6.0",
        "illuminate/support": "~5.6|~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.0",
        "orchestra/testbench": "^3.7 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "HealthEngine\\LaravelUuid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HealthEngine\\LaravelUuid\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpunit": "@php vendor/bin/phpunit --colors=always"
    }
}
